<?xml version="1.0"?>

<robot name="tomi_arm" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- RViz colors -->
  <xacro:include filename="$(find bme_ros_simple_arm)/urdf/materials.xacro" />

  <!-- Transmisions -->
  <xacro:include filename="$(find bme_ros_simple_arm)/urdf/transmission.xacro" />
  <!-- Gazebo plugin -->
  <xacro:include filename="$(find bme_ros_simple_arm)/urdf/tomi_arm.gazebo" />






  <!-- Global reference link -->
  <link name="world"/>

  <joint name="fixed_base" type="fixed">
    <parent link="world"/>
    <child link="link_0"/>
  </joint>

  <!-- Link 0 -->
  <link name="link_0">
    <inertial>
      <mass value="7.57"/>
      <origin xyz="0.0 0.0 0.0957"/>
      <inertia ixx="0.0368" ixy="0.0" ixz="0.0" 
               iyy="0.0368" iyz="0.0"
               izz="0.0273"
      />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.085" length="0.1914"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0957" rpy="0.0 0.0 0"/>
    </collision>
    <visual>
      <geometry>
        <mesh scale = "1 1 1" filename = "package://bme_ros_simple_arm/meshes/Link0.dae"/>
      </geometry>
      <material name="grey"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <!--<gazebo reference="link_0">
    <material>Gazebo/Grey</material>
  </gazebo>-->
  
  <!-- Axis1 - Link 0 - Link 1 -->
  <joint name="Axis1" type="revolute">
    <limit lower="-2.6179" upper="2.6179" effort="5" velocity="7.33"/> <!-- +-150° , 420°/s which is 7.33 rad/s-->
    <parent link="link_0"/>
    <child link="link_1"/>
    <axis xyz="0 0 1"/>
    <!--<origin xyz="0.0 0.0 0.2" rpy="0.0 0.0 0.0"/>-->
    <origin xyz="0.0 0.0 0.1914" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <!-- Link 1 -->
  <link name="link_1">
    <inertial>
      <mass value="6.09"/>
      <origin xyz="0.142 0.0 0.043" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0174" ixy="0.0" ixz="0.0"
               iyy="0.1039" iyz="0.0"
               izz="0.1138"
      />
    </inertial>
    <collision>
      <geometry>
        <box size="0.444 0.164 0.086"/>
      </geometry>
      <origin xyz="0.142 0.0 0.043" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <mesh scale = "1 1 1" filename = "package://bme_ros_simple_arm/meshes/Link1.dae"/>
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <!--<gazebo reference="link_1">
    <material>Gazebo/Orange</material>
  </gazebo>-->

  <!-- Joint 2 - Link 1 - Link 2 -->
  <joint name="Axis2" type="revolute">
    <limit lower="-2.5307" upper="2.5307" effort="5" velocity="12.23"/> <!-- +- 145° which is +- 2.5307 rad , 701°/s which is 12.23 rad/s -->
    <parent link="link_1"/>
    <child link="link_2"/>
    <axis xyz="0 0 1"/>
    <!--<origin xyz="0.290 0.0 0.09" rpy="0.0 0.0 0.0"/>-->
    <origin xyz="0.290 0.0 0.086" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <!-- Link 2  -->
  <link name="link_2">
    <inertial>
      <mass value="9.01"/>
      <origin xyz="0.1075 0.0 0.089" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0398" ixy="0.0" ixz="0.0"
               iyy="0.1764" iyz="0.0"
               izz="0.1686"
      />
    </inertial>
    <collision>
      <geometry>
        <box size="0.451 0.146 0.178"/>
      </geometry>
      <origin xyz="0.1075 0.0 0.089" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <mesh scale = "1 1 1" filename = "package://bme_ros_simple_arm/meshes/Link2.dae"/>
        <!--<cylinder radius="0.05" length="0.2"/>-->
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <!--<gazebo reference="link_2">
    <material>Gazebo/Orange</material>
  </gazebo>-->

  <!-- Joint 3 - Link 2 to Link 3 , fictive link (translation)-->
  <joint name="Axis3" type="prismatic">
    <limit lower="-0.18" upper="0" effort="10" velocity="1.1"/><!-- -180 .. 0 mm , 1100mm/s which is 1.1 m/s -->
    <parent link="link_2"/>
    <child link="link_3"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0.260 0.0 -0.0765" rpy="0.0 0.0 0.0"/>
    <!--<origin xyz="0.260 0.0 0.5" rpy="0.0 0.0 0.0"/>-->
    <dynamics damping="0.0" friction="0.0"/>
  </joint>
  
  
  <link name="link_3">
    <inertial>
      <mass value="0.05"/>
      <origin xyz="0 0 0.1975" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0"
               iyy="0.0001" iyz="0.0"
               izz="0.0001"
      />
    </inertial>
  </link>

  <!--<gazebo reference="link_3">
    <material>Gazebo/Orange</material>
  </gazebo>-->

  


 


  <!-- Joint 4 - Link 3 to Link 4 (rotation)-->
  <joint name="Axis4" type="revolute">
    <limit lower="-6.98" upper="6.98" effort="5" velocity="43.63"/> <!-- +- 400° which is +- 6.98 rad , 2500°/s which is 43.63 rad/s -->
    <parent link="link_3"/>
    <child link="link_4"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <!-- Link 4 -->
  <link name="link_4">
    <inertial>
      <mass value="0.18"/>
      <origin xyz="0.0 0.0 0.1975" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0023" ixy="0.0" ixz="0.0"
               iyy="0.0023" iyz="0.0"
               izz="0.00"
      />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.01" length="0.395"/>
      </geometry>
      <origin xyz="0.0 0.0 0.1975" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
      <mesh scale = "1 1 1" filename = "package://bme_ros_simple_arm/meshes/Link3.dae"/>
        <!--<cylinder radius="0.025" length="0.25"/>-->
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>



  <!-- Link 4 - E -->
  <joint name="EndEffectorFIX" type="fixed">
    <parent link="link_4"/>
    <child link="link_E"/>
  </joint>

  <!-- Link 4 -->
  <link name="link_E">
    <inertial>
      <mass value="0.2"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.0011" ixy="0.0" ixz="0.0"
               iyy="0.0011" iyz="0.0"
               izz="0.0004"
      />
    </inertial>
    <collision>
      <geometry>
        <box size="0.026 0.04 0.0884"/>
      </geometry>
      <origin xyz="0.0 0.0 -0.0442" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <mesh scale = "1 1 1" filename = "package://bme_ros_simple_arm/meshes/M.dae"/>
        <!--<cylinder radius="0.025" length="0.25"/>-->
      </geometry>
      <material name="orange"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>
















<!-- Left finger joint -->
  <joint name="left_finger_joint" type="prismatic">
    <limit lower="0.006" upper="0" effort="5.0" velocity="4.0"/>
    <parent link="link_E"/>
    <child link="left_finger"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.0 0.00 0.00" />
  </joint>

  <!-- Left finger link -->
  <link name="left_finger">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.00009" ixy="0.0" ixz="0.0"
               iyy="0.00009" iyz="0.0"
               izz="0.00002"
      />
    </inertial>
    <collision>
      <geometry>
        <box size=".04 .01 .08"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <mesh scale = "1 1 1" filename = "package://bme_ros_simple_arm/meshes/MP1.dae"/>
      </geometry>
      <material name="blue"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="left_finger">
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <mu1>15</mu1>
    <mu2>15</mu2>
    <fdir1>1 0 0</fdir1>
    <maxVel>1.0</maxVel>
    <minDepth>0.002</minDepth>
    <material>Gazebo/Blue</material>
  </gazebo>













  <!-- Right finger joint -->
  <joint name="right_finger_joint" type="prismatic">
    <limit lower="0" upper="0.006" effort="5.0" velocity="4.0"/>
    <parent link="link_E"/>
    <child link="right_finger"/>
    <axis xyz="0 -1 0"/>
    <origin xyz="0.0 0.0 0.0" />
  </joint>

  <!-- Right finger link -->
  <link name="right_finger">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.00009" ixy="0.0" ixz="0.0"
               iyy="0.00009" iyz="0.0"
               izz="0.00002"
      />
    </inertial>
    <collision>
      <geometry>
        <box size=".04 .01 .08"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
    <visual>
      <geometry>
        <mesh scale = "1 1 1" filename = "package://bme_ros_simple_arm/meshes/MP2.dae"/>
      </geometry>
      <material name="blue"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </visual>
  </link>

  <gazebo reference="right_finger">
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <mu1>15</mu1>
    <mu2>15</mu2>
    <fdir1>1 0 0</fdir1>
    <maxVel>1.0</maxVel>
    <minDepth>0.002</minDepth>
    <material>Gazebo/Blue</material>
  </gazebo>




















  <!-- Gripper camera -->
  <joint type="fixed" name="gripper_camera_joint">
    <origin xyz="0.0 0.0 -0.1" rpy="0 1.5707 0"/>
    <child link="gripper_camera_link"/>
    <parent link="link_E"/>
  </joint>

  <link name='gripper_camera_link'>
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="1.0e-03"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size=".01 .01 .01"/>
      </geometry>
    </visual>

  </link>

  <gazebo reference="gripper_camera_link">
    <material>Gazebo/Red</material>
  </gazebo>

  <joint type="fixed" name="gripper_camera_optical_joint">
    <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707"/>
    <child link="gripper_camera_link_optical"/>
    <parent link="gripper_camera_link"/>
  </joint>

  <link name="gripper_camera_link_optical">
  </link>










  <!-- Table camera -->
  <joint type="fixed" name="table_camera_joint">
    <origin xyz="0.275 0.275 1.0" rpy="0 1.5707 0"/>
    <child link="table_camera_link"/>
    <parent link="world"/>
  </joint>

  <link name='table_camera_link'>
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="1.0e-03"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size=".05 .05 .05"/>
      </geometry>
    </visual>

  </link>

  <gazebo reference="table_camera_link">
    <material>Gazebo/Red</material>
  </gazebo>

  <joint type="fixed" name="table_camera_optical_joint">
    <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707"/>
    <child link="table_camera_link_optical"/>
    <parent link="table_camera_link"/>
  </joint>

  <link name="table_camera_link_optical">
  </link>

























</robot>